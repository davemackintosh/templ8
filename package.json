{
  "name": "templ8m8s",
  "version": "1.0.4",
  "description": "Use strings to generate virtual DOM objects, strings m8s.",
  "main": "./dist/templ8.js",
  "bugs": {
    "url": "https://github.com/davemackintosh/templ8/issues"
  },
  "homepage": "https://github.com/davemackintosh/templ8#readme",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "prepublish": "npm run build",
    "test": "tape tests/all.js | tap-spec",
    "watch": "babel --watch=./index.js --out-file ./dist/templ8.js",
    "build": "babel -s templ8 ./index.js -o ./dist/templ8.js -t babelify"
  },
  "keywords": [
    "string",
    "html",
    "virtualDom",
    "vdom"
  ],
  "author": "davemackintosh",
  "license": "MIT",
  "devDependencies": {
    "babel-cli": "^6.23.0",
    "babel-plugin-transform-flow-strip-types": "^6.21.0",
    "babelify": "^7.3.0",
    "deamdify": "^0.3.0",
    "deglobalify": "^0.2.0",
    "es6ify": "^1.6.0",
    "eslint": "^3.12.0",
    "eslint-plugin-html": "^1.7.0",
    "flow-bin": "^0.37.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.3",
    "watchify": "^3.8.0"
  }
}
